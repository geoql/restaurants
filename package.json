{
  "name": "@geoql/restaurants",
  "version": "1.9.0",
  "description": "Hasura & Nuxt based app to find restaurants near you",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:css",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix && npm run lint:css:fix",
    "lint:js": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|.nuxt|dist|static)/**/}*.{js,ts,vue}\" --ignore-path .gitignore",
    "lint:css": "stylelint '**/*.{css,scss,sass,vue}' --ignore-path .gitignore",
    "lint:css:fix": "stylelint --fix '**/*.{css,scss,sass,vue}' --ignore-path .gitignore",
    "release": "shipjs prepare",
    "release:dry": "shipjs prepare --dry-run",
    "release:auto": "shipjs prepare --yes",
    "prepare": "is-ci || husky",
    "hasura": "hasura --project hasura --skip-update-check",
    "hasura:console": "npm run hasura console",
    "hasura:apply": "npm run hasura migrate apply"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "@turf/area": "^6.5.0",
    "@turf/circle": "^6.5.0",
    "click-outside-vue3": "^4.0.1",
    "idb": "^8.0.0",
    "maplibre-gl": "^4.3.2",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@julr/unocss-preset-forms": "^0.1.0",
    "@nuxt/devtools": "^1.3.3",
    "@nuxtjs/apollo": "^5.0.0-alpha.14",
    "@nuxtjs/plausible": "^1.0.0",
    "@types/geojson": "^7946.0.14",
    "@types/node": "^20.14.2",
    "@types/papaparse": "^5.3.14",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@unocss/eslint-config": "^0.60.4",
    "@unocss/nuxt": "^0.60.4",
    "@unocss/preset-wind": "^0.60.0",
    "@unocss/reset": "^0.60.0",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "@vueuse/core": "^10.10.0",
    "@vueuse/nuxt": "^10.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.9",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-vue": "^9.26.0",
    "geojson": "^0.5.0",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.5",
    "nuxt": "^3.11.2",
    "prettier": "^3.3.1",
    "sass": "^1.77.4",
    "shipjs": "^0.26.3",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.5",
    "unocss-preset-scrollbar": "^0.3.1"
  },
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "url": "git+https://github.com/geoql/restaurants.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/geoql/restaurants/issues"
  },
  "homepage": "https://restaurants.geoql.in/"
}
